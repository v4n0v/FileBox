<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="renameRootElement" prefHeight="66.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.geekbrains.filebox.client.fxcontrollers.NewFolderController">
    <children>
        <TextField fx:id="fieldNewFolder" prefHeight="25.0" prefWidth="275.0" text="NewFolder">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin></TextField>
        <Button fx:id="btnRename" mnemonicParsing="false" onAction="#newFolder" prefHeight="47.0" prefWidth="117.0" styleClass="fontverdabold" stylesheets="@../css/currentStyleCSS.css" text="Create Folder">
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
            </padding>
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
        </Button>
    </children>
</HBox>
